{
    "version": 2,
    "builds": [
      {
        "src": "api/index.php",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/doc/(.*)",
        "dest": "/api/index.php"
      },
      {
        "src": "/(.*)",
        "dest": "/public/index.php"
      }
    ]
  }
  